for (Map.Entry<String, List<Object>> entry : reportDatas.entrySet()) {
    String reportName = entry.getKey();
    List<Object> reportData = entry.getValue();

    JasperDesign jdReport = JRXmlLoader.load(reportDirectory + reportName + TYPE_JRXML);
    compileReport(reportName, jdReport);

    try {
        currentReport = reportParamRepository.findByJrxmlName(reportName);
    } catch (Exception ex) {
        logger.info("Exception occurred during setUpReports for {}: {}", reportName, ex.getMessage(), ex);
    }
}